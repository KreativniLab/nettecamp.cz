			"Ublaboo\\Mailing\\Tests\\": "tests/"
		}
	},
	"require": {
		"php": ">= 7.1",
		"nette/utils": "~2.4 || ~3.0",
		"nette/di": "~2.4 || ~3.0",
		"nette/application": "~2.4 || ~3.0",
		"nette/mail": "~2.4 || ~3.0",
		"latte/latte": "~2.4 || ~3.0"
	},
	"require-dev": {
		"nette/tester": "~2.0",
		"mockery/mockery": "~1.0",
		"tracy/tracy": "^2.4"
	}
